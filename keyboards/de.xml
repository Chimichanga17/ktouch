<?xml version = '1.0' encoding = 'UTF-8'?>
<KeyboardLayout LanguageID="de" >
  <KeyboardFont>Courier</KeyboardFont>
  <KeyDefinitions>
    <KeyDef Slots="1" Row="1" UpperChar="°" >^</KeyDef>
    <KeyDef Slots="2" Row="1" UpperChar="!" >1</KeyDef>
    <KeyDef Slots="3" Row="1" UpperChar="''" >2</KeyDef>
    <KeyDef Slots="2" Row="3" FingerKey="yes" >A</KeyDef>
    <KeyDef Slots="3" Row="3" FingerKey="yes" >S</KeyDef>
    <KeyDef Slots="4" Row="3" FingerKey="yes" >D</KeyDef>
    <KeyDef Slots="5" Row="3" FingerKey="yes" >F</KeyDef>
    <KeyDef Slots="6" Row="3" >G</KeyDef>
    <KeyDef Slots="7" Row="3" >H</KeyDef>
    <KeyDef Slots="8" Row="3" FingerKey="yes" >J</KeyDef>
    <KeyDef Slots="9" Row="3" FingerKey="yes" >K</KeyDef>
    <KeyDef Slots="10" Row="3" FingerKey="yes" >L</KeyDef>
    <KeyDef Slots="11" Row="3" FingerKey="yes" >Ö</KeyDef>
    <KeyDef Slots="12" Row="3" >Ä</KeyDef>
    <KeyDef Slots="13" Row="3" UpperChar="'" >#</KeyDef>
    <ControlKeyDef Slots="1 2" Row="4" UpperChar="Shift" >LeftShift</ControlKeyDef>
  </KeyDefinitions>
  <KeyAccess>
    <KeyPressCombo PressChar="^" KeyChar="^" />
    <KeyPressCombo PressChar="°" KeyChar="^" ControlKeyChar="RightShift" />
    <KeyPressCombo PressChar="a" KeyChar="A" />
    <KeyPressCombo PressChar="A" KeyChar="A" ControlKeyChar="RightShift" />
    <KeyPressCombo PressChar="s" KeyChar="S" />
    <KeyPressCombo PressChar="S" KeyChar="S" ControlKeyChar="RightShift" />
  </KeyAccess>
</KeyboardLayout>
